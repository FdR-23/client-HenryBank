{
  "expo": {
    "name": "HenryBank",
    "slug": "HenryBank",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logoHenryBank.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash2.png",
      "resizeMode": "contain",
      "backgroundColor": "#140152"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "La app necesita acceso a tu c√°mara para tomarte una foto."
        }
      ]
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.soyeison.HenryBank"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
